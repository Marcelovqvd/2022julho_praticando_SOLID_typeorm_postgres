{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "import/no-unresolved":"error",
        "class-methods-use-this":"off",
      "import/prefer-default-export":"off",
      "no-shadow":"off",
      "no-useless-constructor":"off",
      "no-empty-function":"off",
      "lines-between-class-members":"off"
    }
}
